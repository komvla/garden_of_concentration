{"ast":null,"code":"import { computed, defineComponent } from 'vue';\nimport { useStore } from 'vuex';\nimport ButtonField from '../common/ButtonField.vue';\nimport ModalSelectPlant from '../common/ModalSelectPlant.vue';\nexport default defineComponent({\n  components: {\n    ButtonField,\n    ModalSelectPlant\n  },\n  setup() {\n    const store = useStore();\n    const fields = computed(() => store.state.fields);\n    const resetField = fieldIndex => {\n      store.commit('resetField', {\n        fieldIndex\n      });\n    };\n    const canGrow = computed(() => {\n      return fields.value.some(field => field.type !== 'empty');\n    });\n    return {\n      fields,\n      resetField,\n      canGrow\n    };\n  }\n});","map":{"version":3,"mappings":"AAgBA,SAASA,QAAQ,EAAEC,eAAc,QAAS,KAAK;AAC/C,SAASC,QAAO,QAAS,MAAM;AAC/B,OAAOC,WAAU,MAAO,2BAA2B;AACnD,OAAOC,gBAAe,MAAO,gCAAgC;AAG7D,eAAeH,eAAe,CAAC;EAC7BI,UAAU,EAAE;IAACF,WAAW;IAAEC;EAAgB,CAAC;EAC3CE,KAAK,GAAG;IACN,MAAMC,KAAI,GAAIL,QAAQ,EAAE;IAExB,MAAMM,MAAK,GAAIR,QAAQ,CAAC,MAAMO,KAAK,CAACE,KAAK,CAACD,MAAM,CAAC;IAEjD,MAAME,UAAS,GAAKC,UAAU,IAAK;MACjCJ,KAAK,CAACK,MAAM,CAAC,YAAY,EAAE;QAAED;MAAW,CAAC,CAAC;IAC5C,CAAC;IAED,MAAME,OAAM,GAAIb,QAAQ,CAAC,MAAM;MAC7B,OAAOQ,MAAM,CAACM,KAAK,CAACC,IAAI,CAACC,KAAI,IAAKA,KAAK,CAACC,IAAG,KAAM,OAAO,CAAC;IAC3D,CAAC,CAAC;IAGF,OAAO;MAAET,MAAM;MAAEE,UAAU;MAAEG;IAAQ,CAAC;EACxC;AACF,CAAC,CAAC","names":["computed","defineComponent","useStore","ButtonField","ModalSelectPlant","components","setup","store","fields","state","resetField","fieldIndex","commit","canGrow","value","some","field","type"],"sourceRoot":"","sources":["/home/vlad/Escritorio/Programming/garden_of_concentration/src/components/pages/MainGarden.vue"],"sourcesContent":["<template>\n  <div class=\"container content-center mx-auto \">\n    <header class=\"header\">\n    \n    </header>\n    <div class='grid grid-cols-12 gap-4 container py-10'>\n    <table class=\" col-start-4 col-span-6 grid grid-cols-4 gap-4 justify-items-center\">\n        <ButtonField v-for=\"(field, fieldIndex) in fields\" :key=\"fieldIndex\" :idx=\"fieldIndex+1\" :type=\"field.type\" />\n    </table>\n    <!-- <button class=\"col-start-4 col-span-6 m-2 p-5 font-mono text-black font-bold border-4 border-solid bg-lime-400 border-lime-600 rounded-md\" :disabled=\"!canGrow\" @click=\"growNew\">Grow new</button>     -->\n    </div>\n    <ModalSelectPlant :canGrow=\"canGrow\" />\n  </div>\n</template>\n\n<script>\nimport { computed, defineComponent } from 'vue';\nimport { useStore } from 'vuex'; \nimport ButtonField from '../common/ButtonField.vue';\nimport ModalSelectPlant from '../common/ModalSelectPlant.vue';\n\n\nexport default defineComponent({\n  components: {ButtonField, ModalSelectPlant},\n  setup() {\n    const store = useStore();\n\n    const fields = computed(() => store.state.fields);\n\n    const resetField = (fieldIndex) => {\n      store.commit('resetField', { fieldIndex });\n    };\n\n    const canGrow = computed(() => {\n      return fields.value.some(field => field.type !== 'empty');\n    });\n\n\n    return { fields, resetField, canGrow };\n  }\n});\n</script>\n\n<style scoped>\n\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}