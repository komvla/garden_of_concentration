{"ast":null,"code":"import { defineComponent, ref, reactive } from 'vue';\nimport { useRoute } from 'vue-router';\nimport { useStore } from 'vuex';\nexport default defineComponent({\n  setup() {\n    // plant info\n    const route = useRoute();\n    const store = useStore();\n    const plantType = route.params.type;\n    const plantInfo = store.state.typesPlants.find(item => item.type === plantType);\n\n    // timer functionality \n    const buttonRef = ref(null);\n    const timeLeft = reactive({\n      value: 10\n    });\n    const timer = ref(null);\n    const gameState = reactive({\n      started: false,\n      finished: false,\n      won: false\n    });\n    const message = ref('');\n    const showMessage = ref(false);\n    const startTimer = () => {\n      if (!gameState.started) {\n        gameState.started = true;\n        timer.value = setInterval(() => {\n          timeLeft.value--;\n          if (timeLeft.value === 0) {\n            clearInterval(timer.value);\n            gameState.finished = true;\n            gameState.won = false;\n            message.value = 'You lose!';\n            showMessage.value = true;\n          }\n        }, 1000);\n      }\n    };\n    const stopTimer = () => {\n      if (gameState.started && !gameState.finished) {\n        clearInterval(timer.value);\n        message.value = `Time left: ${timeLeft.value} seconds`;\n        showMessage.value = true;\n      }\n    };\n    const resetGame = () => {\n      timeLeft.value = 10;\n      gameState.started = false;\n      gameState.finished = false;\n      gameState.won = false;\n      message.value = '';\n      showMessage.value = false;\n    };\n    const buttonText = computed(() => {\n      if (gameState.finished) {\n        return gameState.won ? 'You win!' : 'You lose!';\n      }\n      return gameState.started ? `${timeLeft.value} seconds left` : 'Click to start';\n    });\n    const showTryAgain = computed(() => {\n      return gameState.finished && !gameState.won;\n    });\n    return {\n      plantInfo,\n      plantType,\n      buttonRef,\n      buttonText,\n      showTryAgain,\n      startTimer,\n      stopTimer,\n      resetGame,\n      message,\n      showMessage\n    };\n  }\n});","map":{"version":3,"mappings":"AAwCA,SAASA,eAAe,EAAEC,GAAG,EAAEC,QAAO,QAAS,KAAK;AACpD,SAASC,QAAO,QAAS,YAAY;AACrC,SAASC,QAAO,QAAS,MAAM;AAG/B,eAAeJ,eAAe,CAAC;EAE7BK,KAAK,GAAG;IACN;IACA,MAAMC,KAAI,GAAIH,QAAQ,EAAE;IACxB,MAAMI,KAAI,GAAIH,QAAQ,EAAE;IAExB,MAAMI,SAAQ,GAAIF,KAAK,CAACG,MAAM,CAACC,IAAI;IAEnC,MAAMC,SAAQ,GAAIJ,KAAK,CAACK,KAAK,CAACC,WAAW,CAACC,IAAI,CAACC,IAAG,IAAKA,IAAI,CAACL,IAAG,KAAMF,SAAS,CAAC;;IAE/E;IACA,MAAMQ,SAAQ,GAAIf,GAAG,CAAC,IAAI,CAAC;IAC3B,MAAMgB,QAAO,GAAIf,QAAQ,CAAC;MAAEgB,KAAK,EAAE;IAAG,CAAC,CAAC;IACxC,MAAMC,KAAI,GAAIlB,GAAG,CAAC,IAAI,CAAC;IACvB,MAAMmB,SAAQ,GAAIlB,QAAQ,CAAC;MACzBmB,OAAO,EAAE,KAAK;MACdC,QAAQ,EAAE,KAAK;MACfC,GAAG,EAAE;IACP,CAAC,CAAC;IACF,MAAMC,OAAM,GAAIvB,GAAG,CAAC,EAAE,CAAC;IACvB,MAAMwB,WAAU,GAAIxB,GAAG,CAAC,KAAK,CAAC;IAE9B,MAAMyB,UAAS,GAAI,MAAM;MACvB,IAAI,CAACN,SAAS,CAACC,OAAO,EAAE;QACtBD,SAAS,CAACC,OAAM,GAAI,IAAI;QACxBF,KAAK,CAACD,KAAI,GAAIS,WAAW,CAAC,MAAM;UAC9BV,QAAQ,CAACC,KAAK,EAAE;UAChB,IAAID,QAAQ,CAACC,KAAI,KAAM,CAAC,EAAE;YACxBU,aAAa,CAACT,KAAK,CAACD,KAAK,CAAC;YAC1BE,SAAS,CAACE,QAAO,GAAI,IAAI;YACzBF,SAAS,CAACG,GAAE,GAAI,KAAK;YACrBC,OAAO,CAACN,KAAI,GAAI,WAAW;YAC3BO,WAAW,CAACP,KAAI,GAAI,IAAI;UAC1B;QACF,CAAC,EAAE,IAAI,CAAC;MACV;IACF,CAAC;IAED,MAAMW,SAAQ,GAAI,MAAM;MACtB,IAAIT,SAAS,CAACC,OAAM,IAAK,CAACD,SAAS,CAACE,QAAQ,EAAE;QAC5CM,aAAa,CAACT,KAAK,CAACD,KAAK,CAAC;QAC1BM,OAAO,CAACN,KAAI,GAAK,cAAaD,QAAQ,CAACC,KAAK,UAAU;QACtDO,WAAW,CAACP,KAAI,GAAI,IAAI;MAC1B;IACF,CAAC;IAED,MAAMY,SAAQ,GAAI,MAAM;MACtBb,QAAQ,CAACC,KAAI,GAAI,EAAE;MACnBE,SAAS,CAACC,OAAM,GAAI,KAAK;MACzBD,SAAS,CAACE,QAAO,GAAI,KAAK;MAC1BF,SAAS,CAACG,GAAE,GAAI,KAAK;MACrBC,OAAO,CAACN,KAAI,GAAI,EAAE;MAClBO,WAAW,CAACP,KAAI,GAAI,KAAK;IAC3B,CAAC;IAED,MAAMa,UAAS,GAAIC,QAAQ,CAAC,MAAM;MAChC,IAAIZ,SAAS,CAACE,QAAQ,EAAE;QACtB,OAAOF,SAAS,CAACG,GAAE,GAAI,UAAS,GAAI,WAAW;MACjD;MACA,